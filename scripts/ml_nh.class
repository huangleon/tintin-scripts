#class {ml_nh} open
#act {^这是一根粗壮光滑的树枝，现在对着正%1方。$}
{
	#if {"%1" == "南"}
	{
        swing vine;
    }
    {
		turn vine;
	}
	look vine;
};
#act {^你顺着树杆爬了上来。$}
{
    #unact {^你顺着树杆爬了上来。$};
    look vine;
};
#act {^你要看什麽}
{
	#unact {^这是一根粗壮光滑的树枝，现在对着正%1方。$};
	southup;
};
#act {^林中小路} {s};
#act {^平原路}
{
	#unact {^林中小路};
	s;
};
#act {^南海大道}
{
	#unact {^平原路};
	unwield all;
	#2 s;
	#class {ml_nh} kill;
}
#class {ml_nh} close