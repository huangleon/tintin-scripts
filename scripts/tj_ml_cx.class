#class {tj_ml_cx} open
#act {^南北大道} {s};
#act {^小山坡西面}
{
	#unact {^南北大道};
	#unact {^小山坡西面};
	#3 s;climb tree;
};
#act {^这是一根粗壮光滑的树枝，现在对着正%1方。$}
{
	#if {"%1" == "西"}
	{
	    swing vine;
	}
	{
	    turn vine;
	}
	look vine;
};
#act {^你顺着树杆爬了上来。$}
{
	#unact {^你顺着树杆爬了上来。$};
	look vine;
};
#act {^你要看什麽}
{
	#unact {^这是一根粗壮光滑的树枝，现在对着正%1方。$};
	#4 w;
	#class {tj_ml_cx} kill;
};
#class {ml} close