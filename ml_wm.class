#class {ml_wm} open;
#act {^这是一根粗壮光滑的树枝，现在对着正%1方。$}
{
	#if {"%1" == "东"}
	{
	    swing vine;
	}
	{
	    turn vine;
	}
	look vine;
};
#act {^你顺着树杆爬了上来。$}
{
	#unact {^你顺着树杆爬了上来。$};
	look vine;
};
#act {^你要看什麽}
{
	#unact {^这是一根粗壮光滑的树枝，现在对着正%1方。$};
	#3 e;
	#class {ml_wm} kill;
};
#class {ml_wm} close;